  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "catalog:common",
    "@nestjs/core": "catalog:common",
    "@nestjs/platform-express": "catalog:common",
    "reflect-metadata": "catalog:common",
    "rxjs": "catalog:common",
    "axios": "catalog:common",
    "async": "^3.2.2",
    "node-fetch": "^2.6.11",
    "cross-spawn": "^7.0.5",
    "body-parser": "^1.20.3",
    "ip": "^2.0.1",
    "@types/express-serve-static-core": "^4.17.37",
    "minimist": "^1.2.6",
    "babel-loader": "^9.1.3",
    "parse-url": "^6.0.1",
    "@babel/traverse": "^7.25.3",
    "decode-uri-component": "^0.2.2",
    "moment": "^2.29.4",
    "parse-path": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "plist": "^3.0.4",
    "fast-xml-parser": "^4.2.4",
    "@react-native/js-polyfills": "^0.74.84",
    "@xmldom/xmldom": "^0.8.7",
    "markdown-it": "^13.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "catalog:common",
    "@eslint/js": "catalog:common",
    "@nestjs/cli": "catalog:common",
    "@nestjs/schematics": "catalog:common",
    "@nestjs/testing": "catalog:common",
    "@swc/cli": "catalog:common",
    "@swc/core": "catalog:common",
    "@types/express": "catalog:common",
    "@types/jest": "catalog:common",
    "@types/node": "catalog:common",
    "@types/supertest": "catalog:common",
    "eslint": "catalog:common",
    "eslint-config-prettier": "catalog:common",
    "eslint-plugin-prettier": "catalog:common",
    "globals": "catalog:common",
    "jest": "catalog:common",
    "prettier": "catalog:common",
    "source-map-support": "catalog:common",
    "supertest": "catalog:common",
    "ts-jest": "catalog:common",
    "ts-loader": "catalog:common",
    "ts-node": "catalog:common",
    "tsconfig-paths": "catalog:common",
    "typescript": "catalog:common",
    "typescript-eslint": "catalog:common"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.12.1"
}
